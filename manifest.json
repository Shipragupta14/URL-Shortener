{
	"name" : "URL Shortener",
	"version": "0.0.1",
    "manifest_version": 2,
    "description":"Reads the URL you selected, shorten it and automatically copies it.",
    "permissions" : [
    	"tabs"
    ],
   
    "browser_action":{
    	"default_icon":{
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
      },
      "default_popup": "popup.html"
    }

 
}

